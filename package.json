		"tailwindcss": "3.3.2",
		"tslib": "2.5.3",
		"typescript": "5.1.3",
		"vite": "4.3.9",
		"vite-imagetools": "4.0.19",
		"vite-plugin-inspect": "0.7.28",
		"vite-plugin-svelte-purgecss": "0.0.3",
