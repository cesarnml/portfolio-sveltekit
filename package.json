{
	"name": "svelte-kit-tutorial",
	"version": "0.0.1",
	"private": true,
	"assets": [
		"node_modules/sharp/**"
	],
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"debug": "NODE_OPTIONS=--inspect ./node_modules/.bin/vite dev",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:e2e": "playwright test",
		"test:ui": "playwright test --ui",
		"ui": "vitest --ui",
		"test:unit": "vitest -- --watch",
		"coverage": "vitest run --coverage",
		"lint": "eslint . --ext .js,.ts,.svelte",
		"format": "prettier --plugin-search-dir . --write .",
		"fix": "npm run format && npm run lint -- --fix",
		"partytown": "partytown copylib static/~partytown",
		"postbuild": "svelte-sitemap --out-dir .vercel/output/static --domain https://cesar-mejia.com",
		"sb:start": "supabase start && pnpm sb:dump && pnpm sb:reset && pnpm sb:seed && pnpm sb:types",
		"sb:dump": "supabase db dump --data-only -f supabase/seed.sql",
		"sb:types": "supabase gen types typescript --local > src/lib/database.types.ts",
		"sb:reset": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/reset.sql",
		"sb:seed": "psql postgresql://postgres:postgres@localhost:54322/postgres < supabase/seed.sql"
	},
	"devDependencies": {
		"@builder.io/partytown": "0.8.0",
		"@floating-ui/dom": "1.2.9",
		"@giscus/svelte": "2.2.8",
		"@lottiefiles/lottie-interactivity": "1.6.2",
		"@playwright/test": "1.34.3",
		"@prisma/client": "4.15.0",
		"@skeletonlabs/skeleton": "1.6.2",
		"@supabase/auth-helpers-sveltekit": "0.10.0",
		"@supabase/supabase-js": "2.24.0",
		"@sveltejs/adapter-vercel": "3.0.1",
		"@sveltejs/kit": "1.20.1",
		"@sveltejs/vite-plugin-svelte": "2.4.1",
		"@tailwindcss/forms": "0.5.3",
		"@tailwindcss/typography": "0.5.9",
		"@testing-library/dom": "9.3.0",
		"@testing-library/jest-dom": "5.16.5",
		"@testing-library/svelte": "3.2.2",
		"@testing-library/user-event": "14.4.3",
		"@types/gtag.js": "0.0.12",
		"@types/jest": "29.5.2",
		"@types/lodash": "4.14.195",
		"@types/rss": "0.0.30",
		"@types/testing-library__jest-dom": "5.14.6",
		"@typescript-eslint/eslint-plugin": "5.59.8",
		"@typescript-eslint/parser": "5.59.8",
		"@vercel/analytics": "1.0.1",
		"@vitest/coverage-c8": "0.31.4",
		"@vitest/ui": "0.31.4",
		"autoprefixer": "10.4.14",
		"cssnano": "6.0.1",
		"dayjs": "1.11.8",
		"eslint": "8.42.0",
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-airbnb-typescript": "17.0.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-import-resolver-typescript": "3.5.5",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-svelte3": "4.0.0",
		"gray-matter": "4.0.3",
		"iconify-icon": "1.0.7",
		"jsdom": "22.1.0",
		"lodash": "4.17.21",
		"lottie-web-light": "1.1.0",
		"mdsvex": "0.10.6",
		"msw": "1.2.1",
		"party-js": "2.2.0",
		"postcss": "8.4.24",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"prettier-plugin-tailwindcss": "0.3.0",
		"prisma": "4.15.0",
		"rehype-autolink-headings": "6.1.1",
		"rehype-slug": "5.1.0",
		"remark-abbr": "1.4.1",
		"remark-code-titles": "0.1.2",
		"remark-external-links": "9.0.1",
		"remark-github": "11.2.4",
		"remark-reading-time": "1.0.1",
		"rss": "1.2.2",
		"sharp": "0.32.1",
		"shiki": "0.14.2",
		"svelte": "3.59.1",
		"svelte-check": "3.4.3",
		"svelte-fragment-component": "1.2.0",
		"svelte-infinite-loading": "1.3.8",
		"svelte-media": "0.1.5",
