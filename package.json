		"@sentry/node": "7.47.0",
		"@sentry/svelte": "7.47.0",
		"@sentry/vite-plugin": "0.5.1",
		"@skeletonlabs/skeleton": "^1.1.0",
		"@supabase/auth-helpers-sveltekit": "0.9.3",
