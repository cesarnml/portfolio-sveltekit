	},
	"engines": {
		"node": "^18.x",
		"npm": "^9.x"
	},
	"type": "module",
	"dependencies": {
		"@sentry/sveltekit": "7.54.0",
		"tailwind-merge": "^2.2.2",
		"@prisma/client": "5.12.1"
	}
}
